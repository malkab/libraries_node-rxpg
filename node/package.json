{
  "name": "@sunntics/rxpg",
  "version": "5.0.9",
  "description": "A RxJS PostgreSQL interface.",
  "scope": "@sunntics",
  "author": {
    "name": "Juan Pedro Perez Alcantara",
    "email": "jp.perez.alcantara@gmail.com",
    "url": "http://me.com"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "keywords": [
    "RxJS",
    "PostgreSQL"
  ],
  "license": "UNLICENSED",
  "homepage": "https://github.com/owner/project#readme",
  "repository": {
    "type": "git",
    "url": "https://gitlab.com/malkab/code-samples.git"
  },
  "private": false,
  "scripts": {
    "build-prod": "yarn clean ; NODE_ENV=production yarn build",
    "build": "yarn clean ; yarn service:build-lib",
    "clean": "rm -Rf build ; rm -Rf dist",
    "install_peer_dep": "npm install --no-save rxjs@7.8.1 @sunntics/ts-utils@8.0.10 pg-native@3.5.0 pg@8.16.0",
    "publish-prod": "yarn publish --patch --no-git-tag-version",
    "quick-test": "npm-run-all --parallel service:watch:quick-test:server service:build",
    "service:build-lib": "webpack --config webpack-production.config.js --progress",
    "service:build": "webpack --progress --watch",
    "service:watch:mocha:server": "nodemon --ext js --watch ./build/ --exec 'mocha --timeout 20000 ./build/mocha.js || true'",
    "service:watch:quick-test:server": "nodemon --inspect=0.0.0.0:9329 \"./build/quicktest.js\" --watch \"./build/quicktest.js\"",
    "start": "npm-run-all --parallel service:watch:mocha:server service:build"
  },
  "devDependencies": {
    "@types/chai": "4.3.20",
    "@types/lodash": "4.17.16",
    "@types/mocha": "9.0.0",
    "@types/node": "22.15.19",
    "@types/pg": "8.15.2",
    "chai": "4.5.0",
    "clean-webpack-plugin": "3.0.0",
    "eslint": "7.32.0",
    "mocha": "9.2.2",
    "nodemon": "2.0.22",
    "npm-run-all": "4.1.5",
    "terser": "5.39.2",
    "terser-webpack-plugin": "5.3.14",
    "ts-loader": "9.5.2",
    "typescript": "5.7.2",
    "webpack": "5.99.8",
    "webpack-cli": "4.10.0",
    "webpack-node-externals": "3.0.0"
  },
  "dependencies": {
    "@sunntics/ts-utils": "8.0.12",
    "lodash": "4.17.21",
    "pg": "8.16.0",
    "pg-native": "3.5.0",
    "rxjs": "7.8.1"
  },
  "packageManager": "yarn@1.22.22+sha512.a6b2f7906b721bba3d67d4aff083df04dad64c399707841b7acf00f6b133b7ac24255f2652fa22ae3534329dc6180534e98d17432037ff6fd140556e2bb3137e"
